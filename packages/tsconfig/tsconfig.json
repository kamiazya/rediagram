{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "rediagram",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "esModuleInterop": true
  },
  "exclude": [
    "**/node_modules/"
  ]
}
